	<script>
		document.addEventListener('DOMContentLoaded', function () {
			const scrollContainer = document.querySelector('.cards');

			function startAutoScroll() {
				scrollContainer.scrollLeft += 1;  // Adjust the speed by changing the increment value
			}

			let scrollInterval = setInterval(startAutoScroll, 20); // Adjust the interval to make scrolling smoother or faster

			// Stop automatic scrolling when user interacts with the cards
			scrollContainer.addEventListener('mouseover', function () {
				clearInterval(scrollInterval);
			});

			scrollContainer.addEventListener('mouseout', function () {
				scrollInterval = setInterval(startAutoScroll, 20);
			});
		});
	</script>

